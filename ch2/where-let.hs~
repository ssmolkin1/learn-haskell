f1 :: Integer
f1 = x * 3 + y
   where x = 3
         y = 4

f2 :: Integer
f2 = x * 5
    where y = 10
          x = 10 * 5 + y

f3 :: Double
f3 = z / x + y
    where x = 7
          y = negate x
          z = y * 10
waxVim :: Double
f x = x / 5
  where x = 2

waxOn ::  Integer
waxOn = x * 5
    where z = 7
          x = y ^ 2
          y = z + 8

triple x = x * 3

waxOff :: Num a => a -> a
waxOff x = triple x ^ 2
    where x = x + 1

troople x = xy

